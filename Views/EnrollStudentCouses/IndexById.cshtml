@model IEnumerable<santisart_app.Models.EnrollStudentCouse>
<div class="right" role="main">
    <h2>@*@Model.First().Students.Student_title) @Model.First().Students.Student_name*@</h2>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>

    <div class="row">
        <div class="col-md-12 col-sm-6 col-xs-12">
            <div class="x_panel">
                <div class="table-responsive">
                    <table class="table table-striped">
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Students.Student_title)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.YearEdu)
                            <th>
                                @Html.DisplayNameFor(model => model.Semester)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.EnrollCouse.CouseTxtId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.EnrollCouse.Course.CourseName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Score)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Grade)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.Enroll_Emp_Pos.EnrEmpPosId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Staff)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Timestamp)
                            </th>
                            <th></th>
                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Students.Student_title)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.YearEdu)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Semester)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.EnrollCouse.CouseTxtId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.EnrollCouse.Course.CourseName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Score)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Grade)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Enroll_Emp_Pos.Employee.EmpName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Staff)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Timestamp)
                                </td>
                                <td>
                                    @Html.ActionLink("Edit", "Edit", new { id = item.EnrollStudentCouseId }) |
                                    @Html.ActionLink("Details", "Details", new { id = item.EnrollStudentCouseId }) |
                                    @Html.ActionLink("Delete", "Delete", new { id = item.EnrollStudentCouseId })
                                </td>
                            </tr>
                        }

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>